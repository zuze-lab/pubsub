{
    "name": "@zuze/pubsub",
    "version": "2.0.0",
    "author": "akmjenkins",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/zuze-lab/pubsub"
    },
    "description": "A functional minimalist pubsub implementation with utilities",
    "keywords": [
        "event",
        "eventbus",
        "pubsub",
        "eventemitter",
        "publish/subscribe",
        "functional",
        "pipe",
        "operators"
    ],
    "main": "build",
    "module": "src",
    "browser": "browser.js",
    "types": "src",
    "scripts": {
        "prepublishOnly": "yarn lint && yarn test && yarn test:typescript && yarn build",
        "clean": "rimraf build",
        "build": "yarn clean && rollup -c ",
        "lint": "eslint src/",
        "rollup": "rimraf build && rollup -c",
        "test:ci": "yarn test:typescript && jest --coverage --coverageReporters=text-lcov | coveralls",
        "test": "jest --testPathPattern=test.js",
        "test:typescript": "jest --testPathPattern=test.ts --config jest.tsconfig.js"
    },
    "files": [
        "build",
        "src",
        "browser.js",
        "operators.js",
        "index.d.ts"
    ],
    "sideEffects": false,
    "devDependencies": {
        "@babel/cli": "^7.10.1",
        "@babel/core": "^7.10.2",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.10.2",
        "@rollup/plugin-babel": "^5.2.1",
        "@rollup/plugin-node-resolve": "^10.0.0",
        "@types/jest": "^25.2.1",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^25.1.0",
        "coveralls": "^3.0.9",
        "eslint": "^6.4.0",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-import": "^2.21.1",
        "eslint-plugin-jest": "^22.17.0",
        "eslint-plugin-prettier": "^3.1.0",
        "jest": "^25.1.0",
        "prettier": "^1.18.2",
        "reselect": "^4.0.0",
        "rollup": "^2.15.0",
        "rollup-plugin-sourcemaps": "^0.6.2",
        "rollup-plugin-terser": "^6.1.0",
        "ts-jest": "^25.3.1",
        "typescript": "^3.6.4"
    }
}